APP_ENV=dev
APP_DEBUG=1
APP_SECRET=ThisTokenIsNotSoSecretSetUsingEnvironmentVariables
TRUSTED_PROXIES=
TRUSTED_HOSTS=

COMPOSER_HOME=/app/var/composer
COMPOSER_CACHE=/app/var/composer/cache
COMPOSER_FUND=0
COMPOSER_HTACCESS_PROTECT=0
COMPOSER_MEMORY_LIMIT=1g
COMPOSER_NO_AUDIT=1

SATIS_CONFIG=/app/satis.json
SATIS_LOG=/app/var/satis

PACKAGE_SECRET=your_secret_here

###> symfony/lock ###
# Choose one of the stores below
# postgresql+advisory://db_user:db_password@localhost/db_name
LOCK_DSN=flock
###< symfony/lock ###
